{
  "plugin": [
    "typedoc-material-theme",
    "typedoc-plugin-missing-exports",
    "typedoc-plugin-rename-defaults"
  ],
  "themeColor": "#252428",
  "entryPoints": ["src"],
  "exclude": [
    "**/*.spec.ts",
    "**/*.test.ts",
    "**/*e2e-spec.ts",
    "test/**",
    "**/node_modules/**"
  ],
  "entryPointStrategy": "expand",
  "out": "docs"
}
